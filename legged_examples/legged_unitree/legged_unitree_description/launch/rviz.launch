<?xml version="1.0" ?>

<launch>
    <!-- visualization config -->
    <arg name="rviz"               default="true" />
    <!-- rviz -->
    <group if="$(arg rviz)">
      <arg name="rvizconfig" default="$(find legged_unitree_description)/rviz/go1.rviz" />
      <node pkg="rviz" type="rviz" name="rviz" args="-d $(arg rvizconfig)" output="screen" />
    </group>
</launch>